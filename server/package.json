{
  "name": "thesis-backend",
  "version": "1.0.0",
  "description": "Backend API for thesis project",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "supabase gen types typescript --local > src/types/supabase.ts",
    "seed": "tsx scripts/seedData.ts",
    "schemas:validate": "tsx src/services/validateSchemas.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@supabase/supabase-js": "^2.54.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "papaparse": "^5.5.3",
    "pdf-parse": "^1.1.1",
    "xlsx": "^0.18.5",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.10",
    "@types/papaparse": "^5.3.16",
    "@types/pdf-parse": "^1.1.5",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
